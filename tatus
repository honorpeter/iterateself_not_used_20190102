[32m+[m[41m    [m
[32m+[m[41m    [m	[32m//‰ªé‰∏â‰∏™Êï∞‰∏≠ÊâæÂà∞ÊúÄÂ∞èÁöÑ[m
[32m+[m[41m    [m	[32mint min(int a, int b, int c) {[m
[32m+[m[41m    [m		[32mint temp = (a < b ? a : b);[m
[32m+[m[41m    [m		[32mreturn (temp < c ? temp : c);[m
[32m+[m[41m    [m	[32m}[m
[32m+[m[41m    [m
[32m+[m[32m    public:[m
[32m+[m[41m    [m	[32mint GetUglyNumber_Solution(int N) {[m
[32m+[m[41m    [m		[32mugly[0] = 1;[m
[32m+[m[41m    [m		[32mint index2 = 0;[m
[32m+[m[41m    [m		[32mint index3 = 0;[m
[32m+[m[41m    [m		[32mint index5 = 0;[m
[32m+[m[41m    [m		[32mint index = 1;[m
[32m+[m[41m    [m		[32mwhile (index < N) {[m
[32m+[m[41m    [m			[32m//Á´û‰∫â‰∫ßÁîü‰∏ã‰∏Ä‰∏™‰∏ëÊï∞[m
[32m+[m[41m    [m			[32mint val = min(ugly[index2] * 2, ugly[index3] * 3, ugly[index5] * 5);[m
[32m+[m[41m    [m
[32m+[m[41m    [m			[32m//Â∞Ü‰∫ßÁîüËøô‰∏™‰∏ëÊï∞ÁöÑindex*ÂêëÂêéÊå™‰∏Ä‰ΩçÔºõ‰∏∫‰ªÄ‰πàÁõ¥Êé•ÂêëÂêéÂä†Â∞±Ë°åÔºü[m
[32m+[m[41m    [m			[32m//‰∏çËÉΩÁî®elseifÔºåÂõ†‰∏∫ÂèØËÉΩÊúâ‰∏§‰∏™ÊúÄÂ∞èÂÄºÔºåËøôÊó∂ÈÉΩË¶ÅÊå™Âä®Ôºõ[m
[32m+[m[41m    [m			[32mif (val == ugly[index2] * 2) {[m
[32m+[m[41m    [m				[32m++index2;[m
[32m+[m[41m    [m			[32m}[m
[32m+[m[41m    [m			[32mif (val == ugly[index3] * 3) {[m
[32m+[m[41m    [m				[32m++index3;[m
[32m+[m[41m    [m			[32m}[m
[32m+[m[41m    [m			[32mif (val == ugly[index5] * 5) {[m
[32m+[m[41m    [m				[32m++index5;[m
[32m+[m[41m    [m			[32m}[m
[32m+[m[41m    [m			[32mugly[index++] = val;[m
[32m+[m[41m    [m		[32m}[m
[32m+[m[41m    [m		[32mint result = ugly[N - 1];[m
[32m+[m[41m    [m		[32mreturn result;[m
[32m+[m[41m    [m	[32m}[m
[32m+[m[32m    };[m
